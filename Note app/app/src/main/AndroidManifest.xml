<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.android.noteIt">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="NOTE APP"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.example.doodooo.Home.splashscreen"
            android:launchMode="singleTop">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.doodooo.Home.MainActivity" />
        <activity android:name="com.example.doodooo.Home.AddEditNoteActivity"
            android:parentActivityName="com.example.doodooo.Home.MainActivity"/>
    </application>

</manifest>